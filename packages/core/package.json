{
  "name": "@echarts-component/vue",
  "version": "0.1.0",
  "description": "Echarts component for Vue",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "author": "hboot <bobolity@163.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.4.1",
  "keywords": [
    "vue",
    "vue3",
    "echarts",
    "components",
    "data-visualization",
    "charts",
    "vue-echarts"
  ],
  "main": "./dist/echarts-component-vue.js.",
  "module": "./dist/echarts-component-vue.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/echarts-component-vue.js",
      "require": "./dist/echarts-component-vue.cjs"
    },
    "./style.css": "./dist/echarts-component-vue.css"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ngd-b/echarts-component.git"
  },
  "homepage": "https://github.com/ngd-b/echarts-component#readme",
  "bugs": {
    "url": "https://github.com/ngd-b/echarts-component/issues"
  },
  "dependencies": {
    "echarts": "5.6.0",
    "lodash": "^4.17.21",
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.0.10",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "typescript": "~5.8.3",
    "unocss": "^66.3.2",
    "vite": "^7.0.2",
    "vite-plugin-dts": "^4.5.4",
    "vue-tsc": "^3.0.1"
  }
}
